<?xml version="1.0"?>

<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
		xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

	<!-- list all the packages being supplied by this jar -->
	<RDF:Seq about="urn:mozilla:package:root">
		<RDF:li resource="urn:mozilla:package:rewindforward"/>
	</RDF:Seq>

	<!-- package information -->
	<RDF:Description about="urn:mozilla:package:rewindforward"
		chrome:name="rewindforward"
		chrome:displayName="Rewind/Fastforward Buttons"
		chrome:author="SHIMODA Hiroshi"
		chrome:authorURL="http://piro.sakura.ne.jp/"
		chrome:settingsURL="chrome://rewindforward/content/about.xul"
		chrome:extension="true"
		chrome:description="Provides &quot;Rewind&quot; and &quot;Fastforward&quot; buttons to the toolbar."/>


	<!-- overlay information -->
	<RDF:Seq about="urn:mozilla:overlays">
		<RDF:li resource="chrome://navigator/content/navigator.xul"/>
		<RDF:li resource="chrome://communicator/content/pref/pref-navigator.xul"/>

		<RDF:li resource="chrome://browser/content/browser.xul"/>
		<RDF:li resource="chrome://global/content/customizeToolbar.xul"/>
	</RDF:Seq>

	<RDF:Seq about="chrome://navigator/content/navigator.xul">
		<RDF:li>chrome://rewindforward/content/navigatorOverlay.xul</RDF:li>
		<RDF:li>chrome://rewindforward/content/buttonsOverlay.xul</RDF:li>
	</RDF:Seq>
	<RDF:Seq about="chrome://communicator/content/pref/pref-navigator.xul">
		<RDF:li>chrome://rewindforward/content/pref-navigator.xul</RDF:li>
	</RDF:Seq>
	<RDF:Seq about="chrome://browser/content/browser.xul">
		<RDF:li>chrome://rewindforward/content/browserOverlay.xul</RDF:li>
		<RDF:li>chrome://rewindforward/content/buttonsOverlay.xul</RDF:li>
	</RDF:Seq>
	<RDF:Seq about="chrome://global/content/customizeToolbar.xul">
		<RDF:li>chrome://rewindforward/content/buttonsOverlay.xul</RDF:li>
	</RDF:Seq>

</RDF:RDF>
